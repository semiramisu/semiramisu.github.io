---
import OptimizedImage from './OptimizedImage.astro';

export interface Props {
  src: string;
  alt: string;
  class?: string;
  width?: number;
  height?: number;
}

const { src, alt, class: className = "", width, height } = Astro.props;
---

<OptimizedImage
  src={src}
  alt={alt}
  class={className}
  width={width}
  height={height}
  loading="lazy"
  format="webp"
/>